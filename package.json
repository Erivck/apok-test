{
  "name": "apok-technical-test",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/server.js",
    "dev-up": "docker compose -f compose.dev.yml up",
    "dev-down": "docker compose -f compose.dev.yml down",
    "prisma:migrate": "npx prisma migrate dev",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d && sleep 1 && npm run prisma:dev:deploy",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up",
    "transpile": "babel ./src --out-dir dist-server",
    "migrate": "npx prisma migrate reset --force",
    "start": "npm run transpile && node dist-server/server.js"
  },
  "author": "Edilson Rivero",
  "license": "ISC",
  "description": "technical test",
  "dependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@prisma/client": "^5.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0"
  },
  "babel":{
    "presets":["@babel/preset-env"]
  }
}
